
page {
  background: #f5f5f5;
}




.searview {
  position: relative;
  padding-right: 130rpx;
  border: 1px solid #e0e0e0;
  border-right: 0;
  border-radius: 10rpx;
  box-sizing: border-box;
  padding-left: 70rpx;
  height: 85rpx;
  top: 20rpx;
    background: #fff;

}

.shasbg {
  padding: 0 40rpx;
}

.searview button {
  width: 130rpx;
  height: 85rpx;
  position: absolute;
  right: 0;
  top: -1rpx;
  background: #05a67b;
  border: none;
  color: #fff;
  font-size: 30rpx;
  line-height: 85rpx;
  border-radius: 0 10rpx 10rpx 0;
}

.searview input {
  font-size: 28rpx;
  color: #555;
  height: 83rpx;
}

.icon-search {
  position: absolute;
  width: 31rpx;
  height: 31rpx;
  background-size: 31rpx auto;
  left: 25rpx;
  top: 27rpx;
  background-image: url(data:image/png;base64,);
}

.searall {
  position: relative;
  height: 83rpx;
  padding-right: 50rpx;
}

.icon-cancel {
  padding: 10rpx 5rpx;
  width: 30rpx;
  height: 30rpx;
  background-size: 30rpx 30rpx;
  right: 5rpx;
  top: 17.5rpx;
  position: absolute;
  background-position: center;
  background-repeat: no-repeat;
  background-image: url(data:image/png;base64,);
}

/* 商品列表 */

.shopConter {
  width: 100%;
  height: 100%;
  display: flex;
  margin-top: 40rpx;
}

.shop {
  width: 50%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
}

.shopItem {
  width: 340rpx;
  height: 450rpx;
  box-sizing: border-box;
  position: relative;
  overflow: hidden;
  margin: 10rpx 0rpx;
  background: #fff;
  border-radius: 10rpx;
}

.ShopImg {
  display: block;
  width: 100%;
  height: 240rpx;
  margin-bottom: 40rpx;
  /* border:1rpx solid #e0e0e0; */
  background: #efefef;
}

.Name {
  margin-bottom: 20rpx;
  width: 100%;
  height: 80rpx;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  padding: 0rpx 20rpx;
  box-sizing: border-box;
}

.Title {
  font-size: 30rpx;
}

.Number {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 50rpx;
  font-size: 28rpx;
  margin-top: 20rpx;
  padding: 0rpx 20rpx;
  box-sizing: border-box;
}

.Color {
  font-size: 36rpx;
  color: red;
  padding-right: 10rpx 10rpx;
}
